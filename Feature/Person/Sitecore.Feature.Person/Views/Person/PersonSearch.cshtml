
@{
    Layout = null;
}

<h2>@Sitecore.Globalization.Translate.TextByDomain(DictionaryDomain, "Contact Search Title")</h2>

<div class="col-sm-12 col-md-12 table-wrapper"">
    <form class="form-inline" action="/Home">
        <div class="form-group">
            <label for="email">@Sitecore.Globalization.Translate.TextByDomain(DictionaryDomain, "Email")</label>
            <input type="text" class="form-control" id="email" name="email">
        </div>
        <div class="form-group">
            <label for="phone">@Sitecore.Globalization.Translate.TextByDomain(DictionaryDomain, "Phone")</label>
            <input type="text" class="form-control" id="phone" name="phone">
        </div>
        <button type="submit" class="btn btn-primary" style="margin-top:24px; width: 100px;">@Sitecore.Globalization.Translate.TextByDomain(DictionaryDomain, "Search")</button>
    </form>
</div>


  
